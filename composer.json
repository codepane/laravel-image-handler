{
    "name": "codepane/laravel-image-handler",
    "description": "Optimize and store images in multiple sizes easily.",
    "type": "library",
    "version": "0.7",
    "keywords": [
        "image",
        "laravel",
        "image-handler"
    ],
    "require": {
        "intervention/image": "^2.7"
    },
    "readme": "README.md",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Codepane\\LaravelImageHandler\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codepane\\LaravelImageHandler\\ImageHandlerServiceProvider"
            ],
            "aliases": {
                "ImageHandler": "Codepane\\LaravelImageHandler\\Facades\\ImageHandler"
            }
        }
    },
    "authors": [
        {
            "name": "Sanjay Prajapati",
            "email": "sanjay14prajapati1998@gmail.com"
        }
    ],
    "prefer-stable": true
}
